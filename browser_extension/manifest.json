{
  "manifest_version": 3,
  "name": "Antivirus Phishing Protection",
  "version": "1.0",
  "description": "Protects against phishing by scanning URLs in real time.",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
